  "bugs": {
    "url": "https://github.com/nyg/dashborg/issues"
  },
  "dependencies": {
    "@prisma/client": "3.13.0",
    "exceljs": "4.3.0",
    "formidable": "2.0.1",
    "next": "12.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-number-format": "4.9.3",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "autoprefixer": "10.4.5",
    "postcss": "8.4.12",
